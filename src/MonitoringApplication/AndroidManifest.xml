<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="smartask.monitoring" android:versionCode="1"
	android:versionName="1.0">
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.INTERNET" />
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<uses-library android:name="com.google.android.maps" />
		<activity android:name=".MainActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ConflictListActivity" android:label="@string/app_name">
		</activity>
		<activity android:name=".ConflictDetailActivity"
			android:label="@string/app_name">
		</activity>
		<activity android:name=".TrackingMap" android:theme="@android:style/Theme.NoTitleBar">
		</activity>
	</application>
</manifest> 